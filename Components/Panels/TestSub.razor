@using Radiate.Client.Services.Store.States
@using Reflow.Interfaces
@inject ILogger<TestSub> Logger
@inherits StoreComponent<Radiate.Client.Services.Store.RootState>

@code {
    
    protected override void OnSubscribed()
    {
        Subscribe<object>(act =>
        {
            Logger.LogInformation($"Received action {act.GetType().FullName}", act);
        });
    }
}